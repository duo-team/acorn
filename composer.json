{
    "name": "duo-team/acorn",
    "description": "DuoTeam Acorn support package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kacper Pruszy≈Ñski",
            "email": "kacper.pruszynski99@gmail.com"
        }
    ],
    "require": {
        "league/fractal": "^0.19.2",
        "roots/acorn": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DuoTeam\\Acorn\\": "src/"
        },
        "files": ["src/Support/helpers.php"]
    },
    "extra": {
        "acorn": {
            "providers": [
                "DuoTeam\\Acorn\\Providers\\DuoTeamServiceProvider",
                "DuoTeam\\Acorn\\Routing\\Ajax\\Providers\\RoutingServiceProvider",
                "DuoTeam\\Acorn\\Database\\Providers\\DatabaseServiceProvider"
            ]
        }
    }
}
